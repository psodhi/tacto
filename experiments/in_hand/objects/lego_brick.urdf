<?xml version="1.0" ?>
<robot name="lego_brick.urdf">
  <link name="baseLink">
    <contact>
      <lateral_friction value="50.0"/>
      <rolling_friction value="0.0001"/>
      <spinning_friction value="0.0001"/>
    </contact>
    <inertial>
      <origin rpy="0 0 0" xyz="0 0 0"/>
       <mass value="250"/>
       <inertia ixx="1" ixy="0" ixz="0" iyy="1.3" iyz="0" izz="0.5"/>
    </inertial>
    <visual>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <geometry>
				<mesh filename="lego_brick.obj" scale=".05 .05 .05"/>
      </geometry>
       <material name="white">
        <color rgba="1 1 1 1"/>
      </material>
    </visual>
    <collision>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <geometry>
	 	<box size=".05 .05 .05"/>
      </geometry>
    </collision>
  </link>
</robot>